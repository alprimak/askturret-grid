{
  "name": "@askturret/grid-landing",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npm run build:demo",
    "build:demo": "cd .. && npm run build && cd demo && npm install --include=dev && npx vite build && cp -r dist ../landing/dist/demo",
    "preview": "astro preview",
    "start": "npx serve dist -s -l $PORT"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.16.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.19"
  }
}
